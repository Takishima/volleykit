{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["src/main.tsx", "e2e/**/*.ts", "src/features/*/index.ts"],
  "project": ["src/**/*.{ts,tsx}", "e2e/**/*.ts"],
  "ignore": ["src/types/**/*.d.ts", "src/api/schema.ts"],
  "ignoreDependencies": [
    "tailwindcss",
    "@volleykit/shared",
    "openapi-typescript",
    "size-limit",
    "prettier"
  ],
  "ignoreBinaries": [
    "tsc",
    "eslint",
    "vite",
    "size-limit",
    "openapi-typescript",
    "changeset",
    "prettier"
  ],
  "ignoreExportsUsedInFile": true,
  "rules": {
    "duplicates": "off"
  }
}
