{
  "name": "@volleykit/mobile",
  "version": "1.12.0",
  "expo": {
    "install": {
      "exclude": ["react"]
    },
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": ["react-native-widgetkit"]
      }
    }
  },
  "description": "VolleyKit React Native mobile app",
  "type": "module",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "prebuild": "expo prebuild",
    "lint": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --max-warnings 0 --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "knip:check": "knip",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/netinfo": "^11.4.0",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "@react-navigation/native-stack": "^7.0.0",
    "@tanstack/react-query": "^5.90.0",
    "@tanstack/react-query-persist-client": "^5.90.0",
    "@volleykit/shared": "*",
    "date-fns": "^4.1.0",
    "expo": "~54.0.0",
    "expo-font": "~14.0.11",
    "expo-calendar": "~15.0.0",
    "expo-constants": "~18.0.0",
    "expo-linking": "~8.0.0",
    "expo-router": "~6.0.22",
    "expo-local-authentication": "~17.0.0",
    "expo-location": "~19.0.0",
    "expo-notifications": "~0.32.0",
    "expo-secure-store": "~15.0.0",
    "expo-status-bar": "~3.0.0",
    "expo-task-manager": "~14.0.0",
    "nativewind": "^4.0.0",
    "react": "^19.2.3",
    "react-native": "0.81.5",
    "react-native-android-widget": "^0.15.0",
    "react-native-safe-area-context": "5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-widgetkit": "^1.0.1",
    "tailwindcss": "^3.4.0",
    "zod": "^3.25.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@eslint/js": "^9.39.0",
    "@types/jest": "^29.5.14",
    "eslint": "^9.39.0",
    "eslint-plugin-import-x": "^4.11.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-security": "^3.0.0",
    "eslint-plugin-sonarjs": "^3.0.0",
    "jest": "^29.0.0",
    "jest-expo": "~54.0.0",
    "knip": "^5.0.0",
    "react-test-renderer": "^19.2.3",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.40.0"
  }
}
