{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["App.tsx", "src/**/*.{ts,tsx}", "app.json"],
  "project": ["src/**/*.{ts,tsx}", "*.{ts,tsx,js}"],
  "ignore": ["src/types/**/*.d.ts", "nativewind-env.d.ts"],
  "ignoreDependencies": [
    "@volleykit/shared",
    "expo-linking",
    "react-native-screens",
    "zod",
    "zustand",
    "expo-router",
    "expo-updates",
    "expo-system-ui",
    "ojp-sdk-next",
    "@babel/core",
    "eslint",
    "jest",
    "jest-expo",
    "react-test-renderer"
  ],
  "ignoreBinaries": [
    "expo",
    "tsc",
    "eslint",
    "jest",
    "knip"
  ],
  "ignoreExportsUsedInFile": true,
  "rules": {
    "duplicates": "off",
    "unlisted": "off",
    "unresolved": "off"
  }
}
