{
  "name": "volleykit-help",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "build": "astro build && npx pagefind --site dist --output-subdir _pagefind",
    "preview": "astro preview",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md,astro}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md,astro}\""
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "astro": "^6.0.0-beta.1",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "pagefind": "^1.4.0",
    "prettier-plugin-astro": "^0.14.1"
  }
}
